<!-- - headers = ['State Name', 'Email']
= CSV.generate_line headers 
- @trackers.each do |user| 
  = CSV.generate_line([user.state_name, user.uid])  -->

def self.to_csv(options = {})
  CSV.generate(options) do |csv|
    csv << column_names
    all.each do |tracker|
      csv << tracker.attributes.values_at(*column_names)
    end
  end
end
